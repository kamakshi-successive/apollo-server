{
  "name": "apollo-server",
  "version": "1.0.0",
  "description": "GraphQL is a syntax that describes how to ask for data, and is generally used to load data from a server to a client. GraphQL has three main characteristics:",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec babel-node src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "nodemon": "nodemon ./src/index.js",
    "lint": "eslint ./src"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "es6": true,
      "browser": true
    },
    "rules": {
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-unused-vars": [
        "warn"
      ],
      "no-var": [
        "off"
      ],
      "one-var": [
        "off"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamakshikumari/apollo-server.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kamakshikumari/apollo-server/issues"
  },
  "homepage": "https://github.com/kamakshikumari/apollo-server#readme",
  "dependencies": {
    "apollo-server-express": "^2.19.1",
    "connect": "^3.7.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.4.0",
    "merge-graphql-schemas": "^1.7.8",
    "qs-middleware": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.8",
    "nodemon": "^2.0.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test",
      "...": "..."
    }
  },
  "type": "module"
}
